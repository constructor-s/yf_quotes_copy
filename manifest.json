{
    "manifest_version": 2,
    "name": "YF Quotes Copy",
    "version": "1.0",
    "description": "Copy watchlist quotes from Yahoo Finance.",
    "homepage_url": "https://github.com/mdn/webextensions-examples/tree/master/bookmark-it",
    "icons": {
      "48": "icons/content-copy.png"
    },
  
    "permissions": [
      "activeTab",
      "clipboardWrite"
    ],
  
    "browser_action": {
      "default_icon": "icons/content-copy.png",
      "default_title": "Copy it!"
    },
  
    "background": {
      "scripts": ["background.js"]
    }
}
